#include<stdio.h>
#include<unistd.h>
#include<string.h>

int main(){
    int pipefd[2];
    char buffer[20];
    pipe(pipefd);
    if (fork()== 0){
        read (pipefd[0],buffer,sizeof(buffer));
        printf("child recieved:%s\n",buffer);

    }else{
        char msg[]="Hello Parent!";
        write (pipefd[1],msg,strlen(msg)+1);

    }
    return 0;
}


