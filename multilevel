include <stdio.h>
#include <unistd.h>
#include <sys/types.h>
int main() {
    pid_t pid;
 
    printf("Program started\n");
 
    pid = fork(); 
 
    if (pid == 0) {
        printf("Child PID: %d, Parent PID: %d\n", getpid(), getppid());
 
        pid_t gpid = fork(); 
        if (gpid == 0) {
            printf("Grandchild PID: %d, Parent PID: %d\n", getpid(), getppid());
        }
    } 
    else if (pid > 0) {
        printf("Parent PID: %d, Child PID: %d\n", getpid(), pid);
    } 
    else {
        printf("Fork failed!\n");
    }
 
    return 0;
}
