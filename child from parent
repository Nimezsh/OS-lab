#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>

int main() {
    pid_t pid;

  
    pid = fork();

    if (pid < 0) {
        printf("Fork failed!\n");
    }
    else if (pid == 0) {
        printf("Child process created!\n");
        printf("Child PID: %d, Parent PID: %d\n", getpid(), getppid());
    }
    else {
        // Parent process
        printf("Parent process running!\n");
        printf("Parent PID: %d, Child PID: %d\n", getpid(), pid);
    }

    return 0;
}
